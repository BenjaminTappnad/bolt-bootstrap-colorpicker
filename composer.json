{
    "name": "BenjaminTappnad/bootstrapcolorpicker",
    "description": "Another colorpicker extension. Based on http://mjolnic.com/bootstrap-colorpicker/ and Raistlfiren/FAIconFieldType",
    "type": "bolt-extension",
    "require": {
        "bolt/bolt": "^3.0.0"
    },
    "keywords": [
      "field",
      "colorpicker",
      "backend",
      "color"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Svante Richter",
            "email": "svante.richter@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BenjaminTappnad\\bootstrapcolorpicker\\": "src"
        }
    },
    "extra": {
        "bolt-class": "BenjaminTappnad\\bootstrapcolorpicker\\BootstrapColorPickerFieldTypeExtension",
        "bolt-assets": "web",
        "bolt-icon": "web/colorpicker-icon.png"
    }
}
