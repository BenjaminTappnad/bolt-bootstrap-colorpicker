{
  "name": "benjamintappnad/bootstrapcolorpicker",
  "description": "Another colorpicker extension. Based on http://mjolnic.com/bootstrap-colorpicker/ and Raistlfiren/FAIconFieldType",
  "type": "bolt-extension",
  "require": {
    "bolt/bolt": "^3.0.0"
  },
  "keywords": [
    "field",
    "colorpicker",
    "backend",
    "color"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Agullana Benjamin",
      "email": "benjaminagullana@parishanghai.fr"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bolt\\Extension\\BenjaminTappnad\\Bootstrapcolorpicker\\": "src"
    }
  },
  "extra": {
    "bolt-class": "Bolt\\Extension\\BenjaminTappnad\\Bootstrapcolorpicker\\BootstrapColorPickerFieldTypeExtension",
    "bolt-assets": "web",
    "bolt-icon": "web/colorpicker-icon.png"
  }
}
